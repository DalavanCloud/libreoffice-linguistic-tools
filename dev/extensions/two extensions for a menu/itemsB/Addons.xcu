<?xml version="1.0" encoding="UTF-8"?>
<oor:node xmlns:oor="http://openoffice.org/2001/registry"
          xmlns:xs="http://www.w3.org/2001/XMLSchema"
          oor:name="Addons" oor:package="org.openoffice.Office">
  <node oor:name="AddonUI">
    <node oor:name="OfficeMenuBar">
        <node oor:name="name.JimK.ATestMenu" oor:op="replace">
            <prop oor:name="Title" oor:type="xs:string">
                <value xml:lang="en">ATest</value>
            </prop>
            <prop oor:name="Target" oor:type="xs:string">
                <value>_self</value>
            </prop>
            <prop oor:name="ImageIdentifier" oor:type="xs:string">
                <value/>
            </prop>
            <node oor:name="Submenu">
                <node oor:name="m04" oor:op="replace">
                    <prop oor:name="URL" oor:type="xs:string">
                        <value>private:separator</value>
                    </prop>
                </node>
                <node oor:name="m05" oor:op="replace">
                    <prop oor:name="URL" oor:type="xs:string">
                        <value>service:name.JimK.LinguisticTools.GrammarSettings?execute</value>
                    </prop>
                    <prop oor:name="Title" oor:type="xs:string">
                        <value/>
                        <value xml:lang="en">Grammar Settings</value>
                        <value xml:lang="fr">Configuration de grammaire</value>
                    </prop>
                    <prop oor:name="Target" oor:type="xs:string">
                        <value>_self</value>
                    </prop>
                    <prop oor:name="Context" oor:type="xs:string">
                        <value>com.sun.star.text.TextDocument</value>
                    </prop>
                </node>
                <node oor:name="m06" oor:op="replace">
                    <prop oor:name="URL" oor:type="xs:string">
                        <value>service:name.JimK.LinguisticTools.GrammarGrabExamples?execute</value>
                    </prop>
                    <prop oor:name="Title" oor:type="xs:string">
                        <value/>
                        <value xml:lang="en">Get Grammar Examples</value>
                        <value xml:lang="fr">Obtenir des exemples de grammaire</value>
                    </prop>
                    <prop oor:name="Target" oor:type="xs:string">
                        <value>_self</value>
                    </prop>
                    <prop oor:name="Context" oor:type="xs:string">
                        <value>com.sun.star.text.TextDocument</value>
                    </prop>
                </node>
            </node>
        </node>
    </node>
  </node>
</oor:node>
